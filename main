<script>
(function () {

  const search = document.getElementById('game-search');
  const gameList = document.getElementById('game-list');
  const gamesArray = Array.from(gameList.getElementsByClassName('game-item-link'));

  gamesArray.sort((a, b) => {
    const nameA = a.dataset.name.replace(/\s+/g,'').toLowerCase();
    const nameB = b.dataset.name.replace(/\s+/g,'').toLowerCase();
    return nameA.localeCompare(nameB);
  }).forEach(game => gameList.appendChild(game));

  search.addEventListener('input', () => {
    const filter = search.value.toLowerCase();
    gamesArray.forEach(game => {
      const name = game.dataset.name.toLowerCase();
      game.classList.toggle('hidden', !name.includes(filter));
    });
  });
})();
</script>
